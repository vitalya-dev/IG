            <kind>1</kind>
            <string>/// Read action number and execute it

switch (keyboard_key) {
    case ord("1"):
    case ord("2"):
    case ord("3"):
    case ord("4"):
    case ord("5"):
    case ord("6"):
    case ord("7"):
    case ord("8"):
    case ord("9"):
    if (keyboard_check_pressed(keyboard_key)) { // Dissable dribling
        var action_index = keyboard_key - ord("0");
        action_index -= 1; // Start from zero.
        if (action_index &gt;= 0 &amp;&amp; action_index &lt; ds_list_size(actions)) {
            // Execute action perform func
            var the_action = ds_list_find_value(actions, action_index)
            var perform_func = asset_get_index(ds_map_find_value(the_action, "perform"))
            script_execute(perform_func, ds_map_find_value(the_action, "value"))
            // And delete it from action list. Not allow call it twice.
            ds_list_delete(actions, action_index)
            // And throw away menu
            instance_destroy()   
        }
    }
}
</string>
